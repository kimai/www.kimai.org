{
    "name": "kimai/www.kimai.org",
    "license": "AGPL-3.0-or-later",
    "type": "project",
    "description": "Kimai - Website",
    "authors": [
        {
            "name": "Kevin Papst",
            "homepage": "https://www.kevinpapst.de"
        }
    ],
    "require": {
        "php": "8.1.*||8.2.*||8.3.*",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "symfony/yaml": "^6.4"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-ctype": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
